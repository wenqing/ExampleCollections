            <name>T0</name>
            <type>MeshNode</type>
            <field_name>initial_temperature</field_name>
        </parameter>
        <parameter>
            <name>P0</name>
            <type>MeshNode</type>
            <field_name>initial_pressure</field_name>
        </parameter>
        <parameter>
            <name>p_Dirichlet_POINT0</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>p_Dirichlet_POINT1</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
