            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>p_Dirichlet_POINT3</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>t_Dirichlet_bottom</name>
            <type>Constant</type>
            <value>170</value>
        </parameter>
        <parameter>
            <name>t_Dirichlet_top</name>
            <type>Constant</type>
            <value>20</value>
        </parameter>
    </parameters>
    <process_variables>
        <process_variable>
            <name>temperature</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>T0</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <geometrical_set>Ra_795_fault</geometrical_set>
                    <geometry>Top</geometry>
                    <type>Dirichlet</type>
                    <parameter>t_Dirichlet_top</parameter>
                </boundary_condition>
                <boundary_condition>
                    <geometrical_set>Ra_795_fault</geometrical_set>
                    <geometry>Bottom</geometry>
                    <type>Dirichlet</type>
                    <parameter>t_Dirichlet_bottom</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
        <process_variable>
            <name>pressure</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>P0</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <geometrical_set>Ra_795_fault</geometrical_set>
                    <geometry>POINT0</geometry>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <geometrical_set>Ra_795_fault</geometrical_set>
                    <geometry>POINT1</geometry>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT1</parameter>
                </boundary_condition>
                <boundary_condition>
                    <geometrical_set>Ra_795_fault</geometrical_set>
                    <geometry>POINT2</geometry>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT2</parameter>
                </boundary_condition>
                <boundary_condition>
                    <geometrical_set>Ra_795_fault</geometrical_set>
                    <geometry>POINT3</geometry>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT3</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_picard_T</name>
            <type>Picard</type>
            <max_iter>10</max_iter>
            <linear_solver>linear_solver_T</linear_solver>
        </nonlinear_solver>
        <nonlinear_solver>
            <name>basic_picard_H</name>
            <type>Picard</type>
            <max_iter>10</max_iter>
            <linear_solver>linear_solver_H</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>linear_solver_T</name>
            <!--lis>-i gmres -restart 60 -p is -is_alpha 0.4 -is_m 6 -tol 1.0e-14  -omp_num_threads 1 -maxiter 3000 </lis-->
            <lis>-i gmres -restart 60 -p ilut -tol 1.0e-14  -omp_num_threads 1 -maxiter 3000 </lis>
            <eigen>
                <solver_type>BiCGSTAB</solver_type>
                <precon_type>ILUT</precon_type>
                <max_iteration_step>3000</max_iteration_step>
                <error_tolerance>1e-10</error_tolerance>
            </eigen>
            <petsc>
                <prefix>T</prefix>
                <!--parameters>-T_ksp_type bcgs -T_pc_type bjacobi -T_ksp_rtol 1e-10 -T_ksp_max_it 3000</parameters-->
                <parameters>-T_ksp_type gmres -T_pc_type  hypre -T_pc_hypre_type boomeramg  -T_ksp_rtol 1e-10 -T_ksp_max_it 3000</parameters>
            </petsc>
        </linear_solver>
        <linear_solver>
            <name>linear_solver_H</name>
            <lis>-i gmres -restart 100 -p  is -is_alpha 0.4 -is_m 6 -tol 1.0e-10    -omp_num_threads 1 -maxiter 3000  </lis>
            <eigen>
                <solver_type>BiCGSTAB</solver_type>
                <precon_type>ILUT</precon_type>
                <max_iteration_step>3000</max_iteration_step>
                <error_tolerance>1e-10</error_tolerance>
            </eigen>
            <petsc>
                <prefix>gw</prefix>
                <!--parameters>-gw_ksp_type bcgs -gw_pc_type bjacobi -gw_ksp_rtol 1e-10 -gw_ksp_max_it 3000</parameters-->
                <parameters>-gw_ksp_type gmres -gw_pc_type    -gw_pc_type  hypre -gw_pc_hypre_type boomeramg  -gw_ksp_rtol 1e-10 -gw_ksp_max_it 3000</parameters>
            </petsc>
        </linear_solver>
    </linear_solvers>
